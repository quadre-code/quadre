        "iconv-lite": "^0.6.3",
        "jasmine-node": "1.11.0",
        "less": "^4.1.3",
        "load-grunt-tasks": "3.5.2",
        "node-abi": "^3.22.0",
        "plugin-error": "^2.0.0",
        "rewire": "1.1.2",
        "rimraf": "^3.0.2",
        "tar": "^6.1.11",
        "tslint": "^5.20.1",
        "typescript": "~3.9.10",
        "vscode-languageserver": "5.3.0-next.1",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.10.0",
        "xmldoc": "0.1.2"
    }
}