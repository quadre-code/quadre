{
  "name": "quadre",
  "productName": "Quadre",
  "description": "Quadre",
  "author": "Quadre Team <ficristo.work@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/quadre-code/quadre",
  "version": "2.0.0-alpha.2",
  "apiVersion": "1.10.0",
  "issues": {
    "url": "https://github.com/quadre-code/quadre/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quadre-code/quadre.git"
  },
  "main": "./dist/index.js",
  "bin": {
    "brackets": "./dist/index.js"
  },
  "build": {
    "appId": "com.squirrel.quadre.Quadre",
    "asar": false,
    "files": [],
    "npmRebuild": true,
    "directories": {
      "buildResources": "build",
      "app": "dist",
      "output": "dist-build"
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "msi": {
      "warningsAsErrors": false
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "category": "Utility;TextEditor;Development;IDE;",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "scripts": {
    "prepush": "npm run test",
    "dep-change": "grunt dep-change",
    "postinstall": "grunt install && npm run build",
    "tsc": "tsc --project app && tsc --project src",
    "build": "rimraf ./dist && gulp copy-src-dist && grunt build && npm run tsc",
    "build-optimized": "npm run build && grunt optimize",
    "test": "npm run tslint",
    "tslint:app": "tslint -c tslint.json --project app/tsconfig.json",
    "tslint:src": "tslint -c src/tslint.json --project src/tsconfig.json",
    "tslint": "npm run tslint:app && npm run tslint:src",
    "dev": "concurrently --kill-others \"gulp watch\" \"tsc --watch --project app\" \"tsc --watch --project src\"",
    "start": "electron .",
    "pack": "npm run build-optimized && build --dir",
    "dist": "npm run build-optimized && build",
    "publish-win": "npm run build-optimized && build -w --publish onTagOrDraft",
    "publish-mac": "npm run build-optimized && build -m --publish onTagOrDraft",
    "publish-linux": "npm run build-optimized && build -l --publish onTagOrDraft",
    "test:prepare:ci": "grunt test && npm run tslint && gulp copy-test-dist",
    "test:unit": "grunt test-integration --suite=unit --spec=all --results=TEST-unit",
    "test:integration": "grunt test-integration --suite=integration --spec=all --results=TEST-integration",
    "test:mainview": "grunt test-integration --suite=mainview --spec=all --results=TEST-mainview",
    "test:livepreview": "grunt test-integration --suite=livepreview --spec=all --results=TEST-livepreview",
    "test:performance": "grunt test-integration --suite=performance --spec=all --results=TEST-performance",
    "test:extension": "grunt test-integration --suite=extension --spec=all --results=TEST-extension",
    "test:all": "grunt test-integration --results=TEST-all"
  },
  "defaultExtensions": {
    "quadre-eslint": "4.1.1"
  },
  "dependencies": {
    "anymatch": "1.3.0",
    "async": "2.3.0",
    "chokidar": "1.6.1",
    "decompress-zip": "0.3.0",
    "electron-updater": "2.17.6",
    "fs-extra": "5.0.0",
    "isbinaryfile": "3.0.2",
    "lodash": "4.17.4",
    "npm": "3.10.9",
    "opn": "4.0.2",
    "portscanner": "^2.1.1",
    "request": "2.81.0",
    "requirejs": "2.3.5",
    "semver": "5.3.0",
    "strip-bom": "3.0.0",
    "temp": "0.8.3",
    "trash": "4.0.1",
    "ws": "2.3.1",
    "xml2js": "0.4.17",
    "yargs": "11.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.0",
    "@types/jquery": "2.0.41",
    "@types/lodash": "4.14.63",
    "@types/react": "16.0.38",
    "@types/react-dom": "16.0.4",
    "@types/ws": "0.0.40",
    "@types/yargs": "11.0.0",
    "concurrently": "3.5.1",
    "cross-spawn": "5.1.0",
    "electron": "~1.8.8",
    "electron-builder": "19.49.2",
    "electron-builder-lib": "19.49.3",
    "electron-builder-squirrel-windows": "19.49.0",
    "electron-packager": "10.1.0",
    "electron-rebuild": "1.6.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.11.1",
    "glob": "7.1.2",
    "grunt": "0.4.5",
    "grunt-cleanempty": "1.0.3",
    "grunt-cli": "0.1.9",
    "grunt-contrib-clean": "0.4.1",
    "grunt-contrib-concat": "0.3.0",
    "grunt-contrib-copy": "0.4.1",
    "grunt-contrib-cssmin": "0.6.0",
    "grunt-contrib-htmlmin": "0.1.3",
    "grunt-contrib-less": "1.4.0",
    "grunt-contrib-requirejs": "0.4.1",
    "grunt-contrib-uglify": "0.2.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-eslint": "20.1.0",
    "grunt-jasmine-node": "0.1.0",
    "grunt-targethtml": "0.2.6",
    "grunt-usemin": "0.1.11",
    "gulp": "3.9.1",
    "gulp-watch": "4.3.11",
    "husky": "0.14.3",
    "jasmine-node": "1.11.0",
    "load-grunt-tasks": "3.5.2",
    "q": "1.4.1",
    "rewire": "1.1.2",
    "rimraf": "2.6.2",
    "tar": "2.2.1",
    "tslint": "5.9.1",
    "typescript": "~3.1.6",
    "typescript-eslint-parser": "^21.0.1",
    "webpack": "2.4.1",
    "xmldoc": "0.1.2"
  }
}
